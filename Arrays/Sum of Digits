#include <stdio.h>
int main() {
    int T, N, sum, digit;
    scanf("%d", &T);
    for (int i = 0; i < T; i++) {  
        scanf("%d", &N);
        sum = 0;
        while (N > 0) {
            digit = N % 10;   // Get the last digit of N
            sum += digit;     // Add the digit to the sum
            N = N / 10;       // Remove the last digit from N
        }
        printf("%d\n", sum);
    }
    return 0;
}

	


