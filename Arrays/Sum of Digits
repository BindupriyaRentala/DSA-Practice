#include <stdio.h>
int main() {
    int T, N, sum, digit;

    // Read the number of test cases
    scanf("%d", &T);

    // Process each test case
    for (int i = 0; i < T; i++) {
        // Read the number N for the current test case
        scanf("%d", &N);
        
        sum = 0;

        // Calculate the sum of digits of N
        while (N > 0) {
            digit = N % 10;   // Get the last digit of N
            sum += digit;     // Add the digit to the sum
            N = N / 10;       // Remove the last digit from N
        }

        // Output the sum of digits for the current test case
        printf("%d\n", sum);
    }

    return 0;
}

